{
  "kind": "collectionType",
  "collectionName": "packs",
  "info": {
    "singularName": "pack",
    "pluralName": "packs",
    "displayName": "Pack",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "Description": {
      "type": "richtext"
    },
    "Image": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "Button": {
      "displayName": "Button",
      "type": "component",
      "repeatable": false,
      "component": "ui.button",
      "required": true
    },
    "Categories": {
      "type": "component",
      "repeatable": false,
      "component": "pack.categories",
      "required": true
    },
    "ModCount": {
      "type": "string",
      "regex": "\\d+"
    },
    "BackgroundColor": {
      "type": "component",
      "repeatable": false,
      "component": "ui.background-color",
      "required": true
    },
    "TargetReleaseDate": {
      "type": "date"
    },
    "ReleaseDate": {
      "type": "date"
    }
  }
}
